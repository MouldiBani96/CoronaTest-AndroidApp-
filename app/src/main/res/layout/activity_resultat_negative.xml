<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="350dp"
    android:layout_height="350dp"
    android:background="@drawable/background_negtivetest"
    android:padding="5dp"
    android:layout_gravity="center">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="match_parent"
            android:layout_height="136dp"
            app:srcCompat="@drawable/tbib" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="110dp"
            android:text="أنت بخير  "
            android:textColor="#E2293F"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="50dp"
            android:text="نسبة المرض :  "
            android:textColor="#302F2F"
            android:textSize="24sp" />
        <Button
            android:id="@+id/idreturn"
            android:background="@drawable/btn_backgroundsignup"
            android:textAllCaps="false"
            android:textSize="20sp"
            android:textColor="#fff"
            android:layout_marginTop="33dp"
            android:layout_marginLeft="120dp"
            android:layout_marginRight="120dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="إعادة"/>



    </LinearLayout>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="190dp"
        android:layout_marginTop="200dp"
        android:text="%"
        android:textColor="#020202"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/resultatneg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="200dp"
        android:text="0"
        android:textColor="#020202"
        android:textSize="24sp" />
</RelativeLayout>
